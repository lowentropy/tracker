<h1>Listing person_graphs</h1>

<table>
  <tr>
    <th>Person</th>
    <th>Graph</th>
    <th>Position</th>
  </tr>

<% for person_graph in @person_graphs %>
  <tr>
    <td><%=h person_graph.person_id %></td>
    <td><%=h person_graph.graph_id %></td>
    <td><%=h person_graph.position %></td>
    <td><%= link_to 'Show', person_graph %></td>
    <td><%= link_to 'Edit', edit_person_graph_path(person_graph) %></td>
    <td><%= link_to 'Destroy', person_graph, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New person_graph', new_person_graph_path %>
