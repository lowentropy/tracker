<h1>New log</h1>

<% form_for(@log) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :measurement_id %><br />
    <%= f.text_field :measurement_id %>
  </p>
  <p>
    <%= f.label :measured_at %><br />
    <%= f.datetime_select :measured_at %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', logs_path %>
