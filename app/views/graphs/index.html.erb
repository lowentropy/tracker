<h1>Listing graphs</h1>

<table>
  <tr>
    <th>Type</th>
    <th>User</th>
    <th>Title</th>
    <th>Ylabel</th>
    <th>Width</th>
    <th>Height</th>
  </tr>

<% for graph in @graphs %>
  <tr>
    <td><%=h graph.type %></td>
    <td><%=h graph.user_id %></td>
    <td><%=h graph.title %></td>
    <td><%=h graph.ylabel %></td>
    <td><%=h graph.width %></td>
    <td><%=h graph.height %></td>
    <td><%= link_to 'Show', graph %></td>
    <td><%= link_to 'Edit', edit_graph_path(graph) %></td>
    <td><%= link_to 'Destroy', graph, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New graph', new_graph_path %>
